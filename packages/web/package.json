{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --ignore-pattern '*.d.ts' --fix",
    "prebuild": "rimraf public && npm run build:server",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:server": "tsc --outDir ./public src/server.ts --esModuleInterop",
    "predev": "rimraf public",
    "dev": "webpack-dev-server --hot --history-api-fallback",
    "start": "cd public && cross-env NODE_ENV=production node server.js",
    "prestats": "webpack --profile --json > public/stats.json",
    "stats": "webpack-bundle-analyzer public/stats.json -s gzip"
  },
  "license": "MIT",
  "resolution": {
    "terser": "4.6.13"
  },
  "dependencies": {
    "antd": "^4.2.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "express": "^4.17.1",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.6.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "ts-loader": "^7.0.2",
    "typed-css-modules": "^0.6.3",
    "typed-css-modules-loader": "^0.0.18",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-dev-server": "^3.10.3",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
